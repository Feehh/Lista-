    <script language="javascript">

      var n1, n2, resultado;
       window.alert("9-Solicitar a digitação de dois valores distintos. Exibir o maior deles..");
      n1 = window.prompt("Digite o 1° valor");
      n2 = window.prompt("Digite o 2° valor");
      resultado = Math.max(n1, n2);
       window.alert("9- O maior valor é " + resultado);
      document.write("9- O maior valor é " + resultado + "<br>");

